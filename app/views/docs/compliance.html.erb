<script nonce="<%= csp_nonce %>">
  document.addEventListener("DOMContentLoaded", function () {
    const links = Array.from(document.querySelectorAll(".docs-nav-link"));
    const indicator = document.getElementById("docs-indicator");
    const headings = Array.from(document.querySelectorAll(".manual-body h2[id]"));

    function setActiveById(id) {
      const active = links.find(a => a.getAttribute("href") === `#${id}`);
      if (!active) return;

      links.forEach(a => a.classList.toggle("active", a === active));

      const top = active.offsetTop;
      const height = active.offsetHeight;
      indicator.style.height = `${height}px`;
      indicator.style.transform = `translateY(${top}px)`;
    }

    if (headings.length) setActiveById(headings[0].id);

    const io = new IntersectionObserver((entries) => {
      const visible = entries
        .filter(e => e.isIntersecting)
        .sort((a, b) => b.intersectionRatio - a.intersectionRatio)[0];

      if (visible && visible.target && visible.target.id) {
        setActiveById(visible.target.id);
      }
    }, { rootMargin: "-20% 0px -70% 0px", threshold: [0.1, 0.25, 0.5] });

    headings.forEach(h => io.observe(h));

    links.forEach(a => {
      a.addEventListener("click", () => {
        const id = (a.getAttribute("href") || "").replace("#", "");
        if (id) setTimeout(() => setActiveById(id), 0);
      });
    });

    const buttons = document.querySelectorAll("[data-copy-btn]");
    buttons.forEach(btn => {
      btn.addEventListener("click", async () => {
        const wrapper = btn.closest("[data-snippet]");
        const codeEl = wrapper ? wrapper.querySelector("[data-code]") : null;
        const text = codeEl ? codeEl.textContent.trim() : "";
        if (!text) return;

        try {
          await navigator.clipboard.writeText(text);
          btn.classList.add("copied");
          const icon = btn.querySelector(".material-icons");
          if (icon) icon.textContent = "check";
          setTimeout(() => {
            btn.classList.remove("copied");
            if (icon) icon.textContent = "content_copy";
          }, 900);
        } catch (e) {
          /* Fallback: seleccionar texto si clipboard no está disponible */
          const range = document.createRange();
          range.selectNodeContents(codeEl);
          const sel = window.getSelection();
          sel.removeAllRanges();
          sel.addRange(range);
        }
      });
    });


  });
</script>