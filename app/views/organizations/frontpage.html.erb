        background-color: #051c2c
    }

    .page-footer {
      padding: 0
    }
      .dark-nav {
      background-color: #050a18 !important;
    }
    .big-font {
      font-size: 18px !important;
      font-weight: 500;
    }

    .extra-height {
    }

    .front-container {
      background-color: #050a18 !important; 
      height: 100%;
      box-sizing: border-box !important;    
    }



    #video-container {
      background-color: yellow;
      heigh: 100px !important;
    }

    #hero {
      min-height: 100vh;
      position: :relative;
    }

      video{
        position: absolute;
        top: 0;
        left: 0;
        height: 100%;
        width: 100%;
        object-fit:cover;
      }


      .capa {
        position: absolute;
        top: 0;
        left: 0;
        height: 100%;
        width: 100%;
      }

    h3 {
      color: #ef4e50;
      font-weight: 1000;
    }

    .login-button, .contact-button {
      border: solid 3px #ef4e50;  
      margin-left: 20px;
    } 

    .contact-button {
      color: white;
      padding: 12px;

    }

    #front-header {
      color: white;
      margin-top: 120px;
    }

    #front-promo {
      width: 70%;
    }

    #front-header * p {
      margin-top: 20px !important;
      line-height: 24px;
      font-size: 18px;
    }

    .front-opacity {
      background-color: rgba(5, 10, 24, 0.2);
    }

    .front-section {
      padding-bottom: 100px;
    }

    ul {
      padding-right: 80px;
    }

    .nav-mobile li {
      margin-top: 20px;
    }

    svg {
      height: 65px;
      vertical-align: middle;
      margin-top: 8px;
      margin-left: 20px;
    }

    .center-section {
      display: flex;
      justify-content: center;      
    } 

    .front-section .center-row {
      display: flex;
      justify-content: center;
    }

    .card-content {
      vertical-align: middle;

    }
    i {
      display: flex;
      vertical-align: middle;
      padding-right: 10px;
    }
    h4 {
      margin-top: 0px;
      border-top: 15px solid #ef4e50;
      padding-top: 25px;
      font-weight: 1000;
    }

    .card-image iframe {
      frameborder: 0;
      height: 400px;
      width: 100%;
      padding-bottom: 10px;
    }

    #us-section * p {
      text-align: center;
    }

    #us-image {
      background-image: url('https://lantiaintelligence.com/img/about/about.jpg');
        background-repeat: no-repeat;
        background-position: center;
        background-size: cover;
    }
    .page-footer{
      background: #ef4e50;
    }

